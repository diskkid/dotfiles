{
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "editor.fontFamily": "'HackGen', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "terminal.integrated.fontFamily": "'HackGen Console for Powerline'",
    "window.zoomLevel": 2,
    "files.exclude": {
        ".vscode": true
    },
    "editor.formatOnSave": true,
    "rust.unstable_features": true,
    "workbench.colorTheme": "Base16 Dark Default",
    "workbench.iconTheme": "material-icon-theme",
    "editor.minimap.enabled": false,
    "workbench.colorCustomizations": {
        "[Ayu Dark]": {
            "foreground": "#999",
            "activityBar.foreground": "#999",
            "sideBar.foreground": "#999",
            "sideBarTitle.foreground": "#999",
            "sideBarSectionHeader.foreground": "#999",
            "tab.activeForeground": "#fff",
            "tab.inactiveForeground": "#999",
            "list.activeSelectionForeground": "#fff",
            "list.inactiveSelectionForeground": "#fff",
            "list.hoverForeground": "#fff",
            "statusBar.foreground": "#999"
        }
    },
    "C_Cpp.clang_format_style": "{BreakBeforeBraces: Attach, IndentWidth: 4}",
    "C_Cpp.clang_format_fallbackStyle": "LLVM",
    "C_Cpp.clang_format_path": "/bin/clang-format",
    "editor.parameterHints.enabled": false,
    "go.inferGopath": true
}
